{
  "name": "@loilo/rx",
  "version": "2.1.3",
  "private": false,
  "description": "A template tag for creating regular expressions",
  "type": "module",
  "unpkg": "dist/rx.js",
  "jsdelivr": "dist/rx.js",
  "main": "./dist/rx.js",
  "module": "./dist/rx.js",
  "exports": {
    "import": "./dist/rx.js"
  },
  "source": "rx.ts",
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsc",
    "pretest": "npm run build",
